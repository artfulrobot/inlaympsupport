<?xml version="1.0"?>
<extension key="inlaympsupport" type="module">
  <file>inlaympsupport</file>
  <name>MPs' support Inlay</name>
  <description>An Inlay providing a list of UK MPs supporting an issue</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Rich Lott / Artful Robot</author>
    <email>forums@artfulrobot.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/artfulrobot/inlaympsupport</url>
    <url desc="Documentation">https://github.com/artfulrobot/inlaympsupport</url>
    <url desc="Support">https://github.com/artfulrobot/inlaympsupport</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-11-21</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.30</ver>
  </compatibility>
  <comments>This is mostly for private use but you may find it a helpful example of an Inlay</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Inlaympsupport</namespace>
  </civix>
</extension>
